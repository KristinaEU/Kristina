@prefix : <http://kristina-project.eu/ontologies/la/action#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix core: <http://kristina-project.eu/ontologies/la/context#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://kristina-project.eu/ontologies/la/action> .

<http://kristina-project.eu/ontologies/la/action> rdf:type owl:Ontology ;
                                                  
                                                  owl:imports <http://kristina-project.eu/ontologies/context> .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://kristina-project.eu/ontologies/la/action#contains

:contains rdf:type owl:ObjectProperty .



###  http://kristina-project.eu/ontologies/la/action#semanticContent

:semanticContent rdf:type owl:ObjectProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://kristina-project.eu/ontologies/la/action#individualType

:individualType rdf:type owl:DatatypeProperty ;
                
                rdfs:range xsd:boolean .



###  http://kristina-project.eu/ontologies/la/action#textualContent

:textualContent rdf:type owl:DatatypeProperty ;
                
                rdfs:range xsd:string .





#################################################################
#
#    Classes
#
#################################################################


###  http://kristina-project.eu/ontologies/la/action#Greeting

:Greeting rdf:type owl:Class ;
          
          rdfs:subClassOf :SpeechAct .



###  http://kristina-project.eu/ontologies/la/action#Inform

:Inform rdf:type owl:Class ;
        
        rdfs:subClassOf :SpeechAct .



###  http://kristina-project.eu/ontologies/la/action#Request

:Request rdf:type owl:Class ;
         
         rdfs:subClassOf :SpeechAct .



###  http://kristina-project.eu/ontologies/la/action#SpeechAct

:SpeechAct rdf:type owl:Class ;
           
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :individualType ;
                             owl:someValuesFrom xsd:boolean
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :textualContent ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :semanticContent ;
                             owl:someValuesFrom core:Situation
                           ] .



###  http://kristina-project.eu/ontologies/la/action#UserAction

:UserAction rdf:type owl:Class ;
            
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :contains ;
                              owl:someValuesFrom :SpeechAct
                            ] .




