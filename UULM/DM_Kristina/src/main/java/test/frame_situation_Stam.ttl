###  http://kristina-project.eu/ontologies/framesituation/core#Age.age

:Age.age rdf:type owl:Class ;
         
         rdfs:subClassOf :FrameElement ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty dul:classifies ;
                           owl:allValuesFrom [ rdf:type owl:Restriction ;
                                               owl:onProperty dul:hasSetting ;
                                               owl:someValuesFrom :AgeFrame
                                             ]
                         ] .



###  http://kristina-project.eu/ontologies/framesituation/core#AgeDescription

:AgeDescription rdf:type owl:Class ;
                
                rdfs:subClassOf :FrameDescription ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dul:defines ;
                                  owl:someValuesFrom :Age.age
                                ] .



###  http://kristina-project.eu/ontologies/framesituation/core#AgeFrame

:AgeFrame rdf:type owl:Class ;
          
          rdfs:subClassOf :FrameSituation ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty dul:satisfies ;
                            owl:onClass :AgeDescription ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty dul:isSettingFor ;
                            owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                 owl:onProperty dul:isClassifiedBy ;
                                                 owl:someValuesFrom :Entity.age
                                               ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty dul:isSettingFor ;
                            owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                 owl:onProperty dul:isClassifiedBy ;
                                                 owl:someValuesFrom :Age.age
                                               ]
                          ] .



