<http://kristina-project.eu/ontologies/framesituation/core#Age.age>
  rdf:type owl:Class ;
  rdfs:subClassOf :FrameElement ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty dul:hasSetting ;
          owl:someValuesFrom :AgeFrame ;
        ] ;
      owl:onProperty dul:classifies ;
    ] ;
.
:AgeDescription
  rdf:type owl:Class ;
  rdfs:subClassOf :FrameDescription ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty dul:defines ;
      owl:someValuesFrom <http://kristina-project.eu/ontologies/framesituation/core#Age.age> ;
    ] ;
.
:AgeFrame
  rdf:type owl:Class ;
  rdfs:subClassOf :FrameSituation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass :AgeDescription ;
      owl:onProperty dul:satisfies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty dul:isSettingFor ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty dul:isClassifiedBy ;
          owl:someValuesFrom <http://kristina-project.eu/ontologies/framesituation/core#Age.age> ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty dul:isSettingFor ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty dul:isClassifiedBy ;
          owl:someValuesFrom <http://kristina-project.eu/ontologies/framesituation/core#Entity.age> ;
        ] ;
    ] ;
.
